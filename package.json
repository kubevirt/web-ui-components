{
  "name": "kubevirt-web-ui-components",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://github.com/kubevirt/web-ui-components",
  "repository": {
    "type": "git",
    "url": "https://github.com/kubevirt/web-ui-components.git"
  },
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "yarn lint-js && yarn lint-css",
    "lint-js": "yarn lint-js-browser && yarn lint-js-node",
    "lint-js-browser": "eslint src --config config/eslint.browser.js",
    "lint-js-node": "eslint config tools --config config/eslint.node.js",
    "lint-css": "stylelint 'src/**/*.css' --config config/stylelint.config.js",
    "prevalidate": "yarn lint",
    "validate": "node tools/runValidations.js",
    "pretest": "yarn validate",
    "test": "rimraf coverage && jest --config config/jest.config.js",
    "test-watch": "yarn test --watch",
    "prebuild": "yarn test --ci",
    "build": "rimraf dist && yarn build-js && yarn build-css",
    "build-js": "babel src --config-file ./config/babel.config.js --out-dir dist --ignore '**/*.test.js,**/setupTest.js'",
    "build-css": "cd src && find . -name '*.css' -exec cp {} ../dist/{} \\;",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "peerDependencies": {
    "patternfly-react": "2.x",
    "prop-types": "15.x",
    "react": "16.5.x",
    "react-dom": "16.5.x"
  },
  "devDependencies": {
    "@babel/cli": "7.x",
    "@babel/core": "7.x",
    "@babel/preset-env": "7.x",
    "@babel/preset-react": "7.x",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.x",
    "babel-jest": "23.x",
    "babel-plugin-transform-class-properties": "6.x",
    "babel-plugin-transform-export-extensions": "6.x",
    "babel-plugin-transform-object-assign": "6.x",
    "babel-plugin-transform-object-rest-spread": "6.x",
    "chalk": "2.x",
    "coveralls": "3.x",
    "enzyme": "3.x",
    "enzyme-adapter-react-16": "1.x",
    "enzyme-to-json": "3.x",
    "eslint": "5.x",
    "eslint-config-standard": "12.x",
    "eslint-config-standard-react": "7.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jest": "21.x",
    "eslint-plugin-node": "7.x",
    "eslint-plugin-promise": "4.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-standard": "4.x",
    "fs-extra": "7.x",
    "jest": "23.x",
    "patternfly-react": "2.x",
    "prop-types": "15.x",
    "react": "16.5.x",
    "react-dom": "16.5.x",
    "recursive-readdir": "2.x",
    "require-all": "3.x",
    "rimraf": "2.x",
    "stylelint": "9.x",
    "stylelint-config-standard": "18.x"
  },
  "engines": {
    "node": ">=8.6.x"
  }
}
