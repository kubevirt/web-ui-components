{
  "name": "kubevirt-web-ui-components",
  "version": "0.1.44-1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/kubevirt/web-ui-components.git"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/kubevirt/web-ui-components",
  "main": "dist/js/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "yarn lint:js:browser && yarn lint:js:node && yarn lint:sass",
    "lint:js:browser": "eslint src --config config/eslint.browser.js",
    "lint:js:node": "eslint config tools --config config/eslint.node.js",
    "lint:sass": "stylelint 'sass/**/*.scss' --config config/stylelint.config.js",
    "prevalidate": "yarn lint",
    "validate": "node tools/runValidations.js",
    "pretest": "yarn validate && shx rm -rf coverage",
    "test": "yarn jest",
    "test:watch": "yarn test --watch",
    "prebuild": "yarn test --ci && shx rm -rf dist",
    "build": "yarn build:babel && yarn build:sass",
    "build:babel": "concurrently \"yarn build:babel:esm && yarn build:babel:umd\" \"yarn build:babel:cjs\"",
    "build:babel:cjs": "babel --config-file ./config/babel.config.js --ignore \"**/tests,./src/cosmos,./src/jest,**/fixtures\" --out-dir dist/js  src --presets=@babel/env",
    "build:babel:esm": "babel --config-file ./config/babel.config.js --ignore \"**/tests,./src/cosmos,./src/jest,**/fixtures\" --out-dir dist/esm src",
    "build:babel:umd": "babel --config-file ./config/babel.config.js --ignore \"**/tests,./src/cosmos,./src/jest,**/fixtures\" --out-dir dist/umd dist/esm --plugins=transform-es2015-modules-umd",
    "build:sass": "shx cp -r sass dist",
    "dev": "shx rm -rf dist && NODE_ENV=development yarn build:babel:cjs && yarn build:sass",
    "jest": "jest --config config/jest.config.js",
    "cosmos": "cosmos --config config/cosmos.config.js",
    "cosmos:export": "shx rm -rf cosmos && NODE_ENV=production cosmos-export --config config/cosmos.config.js",
    "coveralls": "shx cat coverage/lcov.info | coveralls"
  },
  "peerDependencies": {
    "@patternfly/react-charts": "4.x",
    "@patternfly/react-core": "3.x",
    "patternfly-react": "2.x",
    "prop-types": "15.x",
    "react": "16.6.x",
    "react-dom": "16.3.x",
    "react-router-dom": "4.2.x"
  },
  "dependencies": {
    "@patternfly/react-console": "1.x",
    "babel-plugin-transform-es2015-modules-umd": "6.24.1",
    "classnames": "2.x",
    "concurrently": "4.1.1",
    "js-yaml": "3.x",
    "lodash": "4.x",
    "react-dnd": "2.6.x",
    "react-dnd-html5-backend": "5.0.x",
    "react-measure": "2.x",
    "react-responsive": "6.1.x",
    "reactabular-dnd": "8.16.x"
  },
  "devDependencies": {
    "@babel/cli": "7.x",
    "@babel/core": "7.x",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.x",
    "@babel/polyfill": "7.x",
    "@babel/preset-env": "7.x",
    "@babel/preset-react": "7.x",
    "@patternfly/react-charts": "4.x",
    "@patternfly/react-core": "3.x",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.x",
    "babel-jest": "24.x",
    "babel-loader": "8.x",
    "chalk": "2.x",
    "coveralls": "3.x",
    "css-loader": "1.x",
    "enzyme": "3.x",
    "enzyme-adapter-react-16.3": "1.x",
    "enzyme-to-json": "3.x",
    "eslint": "5.x",
    "eslint-config-airbnb": "17.x",
    "eslint-config-prettier": "3.x",
    "eslint-config-standard": "12.x",
    "eslint-config-standard-react": "7.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jest": "21.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-node": "7.x",
    "eslint-plugin-prettier": "2.x",
    "eslint-plugin-promise": "4.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-standard": "4.x",
    "exec-sh": "0.x",
    "file-loader": "2.x",
    "glob": "7.x",
    "html-webpack-plugin": "3.x",
    "jest": "24.x",
    "node-sass": "4.x",
    "patternfly-react": "2.x",
    "prettier": "1.x",
    "prop-types": "15.x",
    "react": "16.6.x",
    "react-cosmos": "4.7.x",
    "react-cosmos-test": "4.7.x",
    "react-dom": "16.3.x",
    "react-router-dom": "4.2.x",
    "require-all": "3.x",
    "sass-loader": "7.x",
    "shx": "0.x",
    "style-loader": "0.x",
    "stylelint": "9.x",
    "stylelint-config-standard": "18.x",
    "stylelint-scss": "3.x",
    "stylelint-selector-bem-pattern": "2.x",
    "webpack": "4.x"
  },
  "engines": {
    "node": ">=8.6.x"
  }
}
