    "@storybook/react": "3.4.x",
    "autoprefixer": "9.x",
    "babel-core": "6.x",
    "babel-loader": "7.x",
    "babel-plugin-transform-object-rest-spread": "6.x",
    "babel-preset-env": "1.x",
    "babel-preset-react": "6.x",
